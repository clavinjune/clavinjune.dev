main.list{display:grid;grid-row-gap:50px;margin:0 20%;height:max-content}@media only screen and (max-width: 480px){main.list{grid-row-gap:35px;margin:-10% 5% 0}}main.list>div.pagination{color:rgba(51,51,51,0.675);display:grid;font-family:"Roboto Condensed",sans-serif;grid-gap:10px;grid-template-columns:repeat(3, max-content);justify-content:center}main.list>div.pagination>span.current{color:#333}article.summary{display:grid;font-family:"Roboto Condensed",sans-serif;grid-row-gap:7.5px;height:max-content}article.summary>a.title{font-size:40px;font-weight:bolder;line-height:48px}@media only screen and (max-width: 480px){article.summary>a.title{font-size:28px;line-height:32px}}article.summary>div.description{-moz-box-orient:vertical;display:-webkit-box;font-family:"Roboto",sans-serif;font-size:20px;line-height:32px;overflow:hidden;text-overflow:ellipsis;-webkit-box-orient:vertical;-webkit-line-clamp:3}@media only screen and (max-width: 480px){article.summary>div.description{font-size:16px;line-height:22px}}article.summary a.readmore{font-family:"Roboto Condensed",sans-serif;font-weight:bolder;line-height:20px;width:max-content}@media only screen and (max-width: 480px){article.summary a.readmore{font-size:16px;line-height:22px}}

